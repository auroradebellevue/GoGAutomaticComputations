_RWS_concat := rec(
           isFSA := true,
        alphabet := rec(
                type := "identifiers",
                size := 10,
              format := "dense",
               names := [a,A,b,B,c,C,d,D,t0,T0]
               ),
          states := rec(
                type := "simple",
                size := 2
               ),
           flags := ["DFA","minimized","BFS","accessible","trim"],
         initial := [1],
       accepting := [2],
           table := rec(
              format := "dense deterministic",
      numTransitions := 20,
         transitions := [[1,1,1,1,1,1,1,1,2,2],
                         [1,1,1,1,1,1,1,1,2,2] 
                        ]
               )
);
